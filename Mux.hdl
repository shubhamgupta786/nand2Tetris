/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=sel , out=nsel );
    And(a=a , b=nsel , out=nselout );
    And(a=b , b=sel , out=selout );
    Or(a= selout, b=nselout , out=out );
}